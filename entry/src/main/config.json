{
  "app": {
    "bundleName": "com.doubley.life",
    "vendor": "example",
    "version": {
      "code": 1000000,
      "name": "1.0.0"
    }
  },
  "deviceConfig": {},
  "module": {
    "package": "com.doubley.life",
    "name": ".MyApplication",
    "mainAbility": "com.doubley.life.MainAbility",
    "deviceType": [
      "phone"
    ],
    "distro": {
      "deliveryWithInstall": true,
      "moduleName": "entry",
      "moduleType": "entry",
      "installationFree": false
    },
    "metaData": {
      "customizeData": [
        {
          "name": "hwc-theme",
          "value": "androidhwext:style/Theme.Emui.NoActionBar",
          "extra": ""
        }
      ]
    },
    "abilities": [
      {
        "skills": [
          {
            "entities": [
              "entity.system.home"
            ],
            "actions": [
              "action.system.home"
            ]
          }
        ],
        "orientation": "unspecified",
        "name": "com.doubley.life.MainAbility",
        "icon": "$media:gift_open",
        "description": "$string:mainability_description",
        "label": "$string:entry_MainAbility",
        "type": "page",
        "launchType": "standard"
      },
      {
        "permissions": [
          "com.doubley.life.DataAbilityShellProvider.PROVIDER"
        ],
        "name": "com.doubley.life.UserDataAbility",
        "icon": "$media:gift_open",
        "description": "$string:userdataability_description",
        "type": "data",
        "uri": "dataability://com.doubley.life.UserDataAbility"
      },
      {
        "orientation": "unspecified",
        "name": "com.doubley.life.LoginAbility",
        "icon": "$media:gift_open",
        "description": "$string:loginregisterability_description",
        "label": "$string:entry_LoginRegisterAbility",
        "type": "page",
        "launchType": "standard"
      },
      {
        "orientation": "unspecified",
        "name": "com.doubley.life.RegisterAbilitySlice",
        "icon": "$media:gift_open",
        "description": "$string:registerabilityslice_description",
        "label": "$string:entry_RegisterAbilitySlice",
        "type": "page",
        "launchType": "standard"
      },
      {
        "orientation": "unspecified",
        "name": "com.doubley.life.RegisterAbility",
        "icon": "$media:gift_open",
        "description": "$string:register_description",
        "label": "$string:entry_Register",
        "type": "page",
        "launchType": "standard"
      },
      {
        "orientation": "unspecified",
        "name": "com.doubley.life.HomeAbility",
